{
  "name": "drishiq",
  "version": "1.0.0",
  "description": "DrishiQ - AI-powered platform with credential protection",
  "private": true,
  "scripts": {
    "dev": "powershell -ExecutionPolicy Bypass -File scripts/start-dev.ps1",
    "dev:clean": "powershell -ExecutionPolicy Bypass -File scripts/pre-dev-cleanup.ps1 && next dev",
    "dev:no-clean": "next dev",
    "clean": "powershell -ExecutionPolicy Bypass -File scripts/clear-next-cache.ps1",
    "kill:node": "powershell -ExecutionPolicy Bypass -File scripts/kill-next-processes.ps1",
    "kill:port": "powershell -ExecutionPolicy Bypass -File scripts/kill-port.ps1",
    "verify:build": "node scripts/verify-build-health.js",
    "build": "next build && (node scripts/fix-opennext-nft.js || true) && (npm run verify:build || true)",
    "build:cf": "opennextjs-cloudflare build",
    "start": "next start",
    "preview": "npm run build:cf && opennextjs-cloudflare preview",
    "deploy": "npm run build:cf && opennextjs-cloudflare deploy",
    "deploy:wrangler": "npm run build:cf && wrangler deploy",
    "deploy:preprod": "npm run build:cf && wrangler deploy --env preprod",
    "deploy:prod": "npm run build:cf && wrangler deploy --env prod",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest --setupFilesAfterEnv=./jest.setup.js",
    "test:crypto": "jest tests/crypto.test.js",
    "test:replay": "jest __tests__/replay-engine.test.ts --setupFilesAfterEnv=./jest.setup.js",
    "test:ddsa": "jest lib/ddsa/__tests__ __tests__/integration --setupFilesAfterEnv=./jest.setup.js",
    "test:watch": "jest --watch --setupFilesAfterEnv=./jest.setup.js",
    "test:integration": "jest --config jest.config.js --testEnvironment=node __tests__/integration --setupFilesAfterEnv=./jest.setup.js",
    "smoke:replay": "node scripts/smoke/replay-smoke-test.js",
    "smoke:replay:ts": "tsx scripts/smoke/replay-smoke-test.ts",
    "backfill:dry-run": "node scripts/backfill_encrypt_sensitive_fields.js --dry-run",
    "backfill": "node scripts/backfill_encrypt_sensitive_fields.js",
    "worker": "tsx scripts/run-ai-jobs-worker.ts",
    "optimize:images": "node scripts/optimize-images.js",
    "optimize:images:dry": "node scripts/optimize-images.js --dry-run",
    "optimize:avatars": "node scripts/optimize-images.js --folder=avatar",
    "analyze": "next build"
  },
  "keywords": [
    "drishiq",
    "ai",
    "chat",
    "crypto",
    "encryption"
  ],
  "author": "DrishiQ Team",
  "license": "MIT",
  "dependencies": {
    "omnivyra-main": "file:packages/omnivyra-main",
    "@aws-sdk/client-s3": "^3.934.0",
    "@aws-sdk/s3-request-presigner": "^3.934.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.78.0",
    "@tanstack/react-virtual": "^3.13.18",
    "@types/pg": "^8.15.6",
    "@upstash/redis": "^1.32.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "firebase": "^12.5.0",
    "framer-motion": "^12.23.24",
    "ioredis": "^5.8.2",
    "jose": "^6.1.2",
    "jspdf": "^4.1.0",
    "lucide-react": "^0.552.0",
    "next": "^15.5.6",
    "openai": "^6.7.0",
    "pg": "^8.16.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.4.1",
    "uuid": "^10.0.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.0.0",
    "@opennextjs/cloudflare": "^1.13.0",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.24",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.0.0",
    "eslint-config-next": "^15.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "sharp": "^0.33.0",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "wrangler": "^4.49.0"
  },
  "optionalDependencies": {
    "argon2": "^0.31.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
